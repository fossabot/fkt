{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["dist/**", ".next/**", "build/**", ".cache/**"],
			"dependsOn": ["^build"]
		},
		"test": {
			"outputs": [],
			"dependsOn": []
		},
		"lint": {
			"outputs": []
		},
		"deploy": {
			"dependsOn": ["build", "test", "lint"],
			"outputs": []
		}
	}
}
